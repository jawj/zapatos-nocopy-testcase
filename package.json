{
  "name": "testcase",
  "version": "0.1.0",
  "description": "Testcase for no-copy branch of Zapatos",
  "author": "George MacKerron <george@mackerron.co.uk>",
  "main": "index.js",
  "//": "These packages are currently held back because of breakage/incompatibility: jest",
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^13.13.5",
    "@types/pg": "^7.14.3",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "pg": "^8.3.0",
    "zapatos": "^2.0.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/supertest": "^2.0.9",
    "jest": "^25.5.4",
    "jest-circus": "^26.0.1",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "source env/dev; nodemon --watch 'src/**/*.ts' --exec node --inspect -r ts-node/register src/index.ts",
    "test": "source env/test; jest",
    "updateSchema": "source env/dev; npx --no-install zapatos",
    "dumpSchema": "source env/dev; pg_dump --no-owner -s ${DATABASE_URL} > schema/$(date '+%Y-%m-%d_%H%M').psql"
  },
  "license": "MIT"
}
